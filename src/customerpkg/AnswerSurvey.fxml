<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="650.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="customerpkg.AnswerSurveyController">
   <children>
      <Label layoutX="50.0" layoutY="42.0" text="Answer Surveys">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="73.0" layoutY="119.0" text="Select Survey:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ComboBox fx:id="surveysCombobox" layoutX="185.0" layoutY="117.0" prefHeight="25.0" prefWidth="166.0" />
      <Button fx:id="loadQuestionButton" layoutX="448.0" layoutY="117.0" mnemonicParsing="false" onAction="#loadQuestionsOnClick" text="Load Questions">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label fx:id="questionsLabel" layoutX="78.0" layoutY="188.0" prefHeight="17.0" prefWidth="575.0" text="Question: ">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="answersTextfield" layoutX="177.0" layoutY="237.0" prefHeight="25.0" prefWidth="480.0" />
      <Label layoutX="82.0" layoutY="238.0" text="Answer:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="284.0" layoutY="306.0" mnemonicParsing="false" onAction="#nextQuestionOnClick" text="Submit Answer">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="470.0" layoutY="308.0" mnemonicParsing="false" onAction="#submitSurveyOnClick" text="Submit Survey">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
